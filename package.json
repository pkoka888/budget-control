{
  "name": "budget-control",
  "version": "1.0.0",
  "description": "Complete setup and research documentation for Maybe Finance personal finance app with Docker, CSV import support, and comprehensive GitHub repository catalog.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:accessibility": "playwright test --project=accessibility",
    "test:api": "playwright test --project=api",
    "test:performance": "playwright test --project=performance",
    "test:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:ci": "playwright test --project=chromium",
    "report": "playwright show-report",
    "install-browsers": "playwright install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.56.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17"
  }
}
